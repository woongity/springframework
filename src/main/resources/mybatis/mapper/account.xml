<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.account">
	<select id="selectAll" resultType="account">
		Select ano, balance from account
	</select>
	<select id="selectByAno" parameterType="int" resultType="account">
		Select ano, balance from account where ano=#{ano}
	</select>
	<update id="updateBalance" parameterType="account">
		update account SET
	</update>
</mapper>